{
  "version": "0.2",
  "allowCompoundWords": true,
  "dictionaries": [
    "companies",
    "css",
    "dotnet",
    "docker",
    "en_us",
    "filetypes",
    "fonts",
    "html",
    "makefile",
    "misc",
    "node",
    "npm",
    "people-names",
    "powershell",
    "python",
    "shell",
    "softwareTerms",
    "typescript"
  ],
  "ignoreRegExpList": ["devcontainer-features", "extension-recommendations"],
  "words": [
    "Nate",
    "Scherer",
    "natescherer",
    // Author names
    "abhishek",
    "exelban",
    // Per-Repo Terms
    "chezmoi"
  ],
  "patterns": [
    {
      "name": "devcontainer-features",
      "pattern": "\"features\": {(.|\\n)*}"
    },
    {
      "name": "extension-recommendations",
      "pattern": "\"recommendations\": \\[(.|\\n)*]"
    }
  ],
  "suggestionsTimeout": 5000
}
